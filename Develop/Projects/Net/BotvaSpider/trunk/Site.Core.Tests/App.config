<?xml version="1.0" encoding="utf-8" ?>
<configuration>

  <configSections>
    <sectionGroup name="webMoneyConfiguration">
      <section name="applicationInterfaces" type="WebMoney.XmlInterfaces.Settings.ConfigurationHandler, WebMoney.XmlInterfaces" />
    </sectionGroup>
    <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net" />
  </configSections>


  <webMoneyConfiguration>
    <applicationInterfaces>
      <authorizationMode>Light</authorizationMode>
      <keeperLight>
        <rawData></rawData>
      </keeperLight>
      <requestNumber>
        <startTime>01.01.2009 00:00:00</startTime>
        <period>1000</period>
      </requestNumber>
    </applicationInterfaces>
  </webMoneyConfiguration>

  <log4net debug="true">

    <appender name="FileAppender" type="log4net.Appender.RollingFileAppender">
      <lockingModel type="log4net.Appender.FileAppender+MinimalLock"/>
      <file value="Logs\WebMoney.XmlInterfaces.log" />
      <appendToFile value="true" />
      <rollingStyle value="Size" />
      <maxSizeRollBackups value="10" />
      <datePattern value="dd-MM-yyyy HH:mm" />
      <maximumFileSize value="1MB" />
      <staticLogFileName value="true" />
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%-5p [%d] [%r] [%C.%M] %m%n"/>
      </layout>
      <encoding value="UTF-8"/>
    </appender>

    <appender name="UdpAppender" type="log4net.Appender.UdpAppender">
      <lockingModel type="log4net.Appender.UdpAppender+MinimalLock"/>
      <param name="RemoteAddress" value="127.0.0.1" />
      <param name="RemotePort" value="2322" />
      <layout type="log4net.Layout.XmlLayoutSchemaLog4j, log4net" />
    </appender>

    <root>
      <priority value="ALL" />
      <appender-ref ref="FileAppender" />
      <appender-ref ref="UdpAppender" />
    </root>

    <!-- http://logging.apache.org/log4net/release/config-examples.html -->

  </log4net>

  <appSettings>
    <add key="log4net.Internal.Quiet" value="true" />
    <add key="REAL-MODE" value="True" />


    <add key="random" value="k2lkkkf23"/>
    <add key="ShowExceptions" value="true"/>
    <add key="JsDebugMode" value="false"/>
    <add key="WebMoneyReaderEnabled" value="false"/>
    <add key="WebMoneyReaderPurse" value="R280069392429"/>
    <add key="WebMoneyReaderInterval" value="10"/>
    <add key="AppVersion" value="1.0.0.7"/>
  </appSettings>
  
  <connectionStrings>
    <add name="default" connectionString="Data Source=.;Initial Catalog=Botva;Persist Security Info=True;User ID=Botva;Password=Botva"/>
  </connectionStrings>
</configuration>