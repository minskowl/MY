<UserControl x:Class="EffectiveSoft.SilverlightDemo.Objects.Car"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" 
    Width="31" Height="65">
    <Canvas>
        <Image x:Name="Skin" Width="31" Height="65"  VerticalAlignment="Top" Source="blueCar.png" Canvas.Top="-32" Canvas.Left="-15"    >
            <Image.RenderTransform>
                <RotateTransform Angle="0" CenterX="15" CenterY="30"/>
            </Image.RenderTransform>
        </Image>
        
    </Canvas>

</UserControl>
